{
  "name": "image-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:standalone": "npx dotenv-cli -v NEXT_OUTPUT=standalone -- next build && ncp ./public ./.next/standalone/public && ncp ./.next/static ./.next/standalone/.next/static",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "clean": "pnpm clean:next & pnpm clean:node_modules",
    "clean:next": "rimraf .next .open-next .sst",
    "clean:node_modules": "rimraf node_modules",
    "drizzle:generate": "drizzle-kit generate:pg --out drizzle/migrations --schema drizzle/schema.ts",
    "drizzle:migrate": "npx dotenv -e .env -- node drizzle/migrate.mjs",
    "sst:run": "npx dotenv -e .env -v SKIP_ENV_VALIDATION=true -- sst bind next dev",
    "sst:dev": "npx dotenv -e .env -v SKIP_ENV_VALIDATION=true -- npx sst dev",
    "sst:deploy": "npx dotenv -e .env -v SKIP_ENV_VALIDATION=true -- npx sst deploy --stage prod",
    "sst:deploy:ci": "npx dotenv -v SKIP_ENV_VALIDATION=true -- npx sst deploy --stage prod",
    "sst:remove": "npx dotenv -e .env -v SKIP_ENV_VALIDATION=true -- npx sst remove",
    "typecheck": "npx tsc --noEmit",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.395.0",
    "@clerk/nextjs": "^4.23.5",
    "@clerk/themes": "^1.7.5",
    "@headlessui/react": "^1.7.17",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@tanstack/react-query": "^4.35.3",
    "@trpc/client": "^10.38.2",
    "@trpc/next": "^10.38.2",
    "@trpc/react-query": "^10.38.2",
    "@trpc/server": "^10.38.2",
    "@upstash/ratelimit": "^0.4.4",
    "@upstash/redis": "^1.22.0",
    "blurhash": "^2.0.5",
    "color": "^4.2.3",
    "contrast-color": "^1.0.1",
    "dayjs": "^1.11.9",
    "drizzle-orm": "^0.28.6",
    "fast-average-color": "^9.4.0",
    "framer-motion": "^10.16.4",
    "jotai": "^2.4.2",
    "nanoid": "^4.0.2",
    "next": "^13.4.19",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.1.0",
    "openai": "^4.6.0",
    "pg": "^8.11.3",
    "postgres": "^3.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "react-intersection-observer": "^9.5.2",
    "react-loader-spinner": "^5.4.5",
    "sharp": "^0.32.5",
    "superjson": "1.13.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@clerk/types": "^3.51.0",
    "@types/color": "^3.0.4",
    "@types/contrast-color": "^1.0.1",
    "@types/eslint": "^8.44.2",
    "@types/next-pwa": "^5.6.4",
    "@types/node": "^20.6.0",
    "@types/pg": "^8.10.2",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "autoprefixer": "^10.4.15",
    "aws-cdk-lib": "2.91.0",
    "constructs": "10.2.69",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.19",
    "ncp": "^2.0.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "sst": "2.24.11",
    "tailwindcss": "^3.3.3",
    "tsx": "^3.12.9",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.15.0"
  },
  "pnpm": {
    "overrides": {
      "@aws-sdk/config-resolver@3.374.0": "npm:@smithy/config-resolver@^2.0.7",
      "@aws-sdk/middleware-retry@3.374.0": "npm:@smithy/middleware-retry@^2.0.9",
      "@aws-sdk/smithy-client@3.374.0": "npm:@smithy/smithy-client@^2.0.7",
      "workbox-cacheable-response@6.6.0": "npm:workbox-background-sync@6.6.0",
      "sourcemap-codec@1.4.8": "npm:@jridgewell/sourcemap-codec@^1.4.15"
    },
    "allowedDeprecatedVersions": {
      "querystring": "0.2.0",
      "rollup-plugin-terser": "7.0.2"
    }
  }
}
